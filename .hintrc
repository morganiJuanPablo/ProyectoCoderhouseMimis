{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "box-decoration-break"
        ]
      }
    ],
    "disown-opener": "off",
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "detect-css-reflows/composite": "off",
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ],
    "no-inline-styles": "off",
    "detect-css-reflows/layout": "off",
    "detect-css-reflows/paint": "off",
    "axe/aria": [
      "default",
      {
        "aria-valid-attr-value": "off"
      }
    ]
  }
}